{
    "compilerOptions": {
        "module": "es6",
        "target": "ES6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "rootDir": ".",
        "strict": true,
        "sourceMap": true,
        "allowJs": true,
        "declaration": true,
        "paths": {
            "@sdkSrc/*": [
                "earthsdk/earthsdk3/src/*"
            ],
            "@czmSrc/*": [
                "earthsdk/earthsdk3-cesium/src/*"
            ],
            "@ueSrc/*": [
                "earthsdk/earthsdk3-ue/src/*"
            ]
        }
    },
    "lib": [
        "es2015"
    ],
    "include": [
        "earthsdk/*/src",
        "demos/*/src"
    ]
}
